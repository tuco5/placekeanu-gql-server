{
  "name": "placekeanu-gql-server",
  "version": "1.0.0",
  "description": "graphql server to serve keanu reaves pictures, astraseneca code challenge",
  "main": "index.js",
  "scripts": {
    "postinstall": "npm run generate",
    "compile": "tsc",
    "dev": "ts-node-dev --respawn ./src/index.ts",
